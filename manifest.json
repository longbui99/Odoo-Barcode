{
    "name": "Odoo Barcode",
    "description": "Build an Extension!",
    "version": "2.2.1",
    "manifest_version": 3,
    "icons": {
        "16": "static/logo.png",
        "32": "static/logo.png",
        "48": "static/logo.png",
        "128": "static/logo.png"
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "windows": "Alt+1",
                "linux": "Alt+1",
                "mac": "Command+1"
            }
        }
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "static/logo.png",
            "32": "static/logo.png"
        }
    }
}