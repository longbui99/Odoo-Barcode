{
    "name": "Odoo Barcode",
    "description": "Build an Extension!",
    "version": "2.1",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "windows": "Alt+1",
                "linux": "Alt+1",
                "mac": "Command+1"
            }
        }
    },
    "action": {
        "default_popup": "index.html"
    }
}